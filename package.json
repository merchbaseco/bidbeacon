    "clsx": "^2.1.1",
    "csstype": "^3.2.3",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.7",
    "pino-pretty": "^13.0.0",
    "jotai": "^2.16.0",
