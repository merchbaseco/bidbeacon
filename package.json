    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "bottleneck": "^2.19.5",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "drizzle-orm": "^0.44.7",
    "fastify": "^5.5.0",
    "pg-boss": "12.5.2",
    "pino": "^10.1.0",
    "postgres": "^3.4.7",
    "tslib": "^2.8.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@base-ui-components/react": "^1.0.0-rc.0",
    "@biomejs/biome": "^2.2.2",
    "@fontsource/aclonica": "^5.2.8",
    "@fontsource/fira-code": "^5.2.7",
    "@merchbaseco/icons": "^1.1.0",
